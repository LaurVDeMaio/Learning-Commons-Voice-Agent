{
  "name": "@inworld/runtime",
  "version": "0.5.2",
  "license": "SEE LICENSE IN LICENSE.md and LICENSE-CPP-BINARIES.md",
  "exports": {
    ".": "./build/index.js",
    "./common": "./build/common/export.js",
    "./graph": "./build/graph/export.js",
    "./graph/nodes": "./build/graph/dsl/nodes/builtin/export.js",
    "./graph/components": "./build/graph/dsl/components/export.js",
    "./core/text_processing": "./build/core/text_processing/export.js",
    "./core": "./build/core/export.js",
    "./primitives/embedder": "./build/primitives/embedder/export.js",
    "./primitives/intent": "./build/primitives/intent/export.js",
    "./primitives/knowledge": "./build/primitives/knowledge/export.js",
    "./primitives/llm": "./build/primitives/llm/export.js",
    "./primitives/memory": "./build/primitives/memory/export.js",
    "./primitives/ner": "./build/primitives/ner/export.js",
    "./primitives/safety": "./build/primitives/safety/export.js",
    "./primitives/stt": "./build/primitives/stt/export.js",
    "./primitives/tts": "./build/primitives/tts/export.js",
    "./primitives/vad": "./build/primitives/vad/export.js",
    "./telemetry": "./build/telemetry/export.js"
  },
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "inworld",
    "ai",
    "asr",
    "character",
    "npc",
    "speech recognition",
    "text to speech",
    "tts",
    "voice"
  ],
  "author": {
    "name": "Inworld AI",
    "url": "https://www.inworld.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inworld-ai/inworld-framework-nodejs.git"
  },
  "scripts": {
    "binaries:download": "node ./scripts/download-unzip-binaries.cjs",
    "postinstall": "npm run binaries:unzip",
    "binaries:unzip": "node ./scripts/binaries-unzip.cjs",
    "build": "tsc -p .",
    "release:pack": "yarn build && node ./scripts/pack.cjs",
    "lint": "eslint \"{src,templates,__tests__,sandbox}/**/*.{js,ts,tsx,jsx}\" --fix",
    "lint:check": "eslint \"{src,templates,__tests__,sandbox}/**/*.{js,ts,tsx,jsx}\"",
    "prettier": "prettier --write \"{src,templates,__tests__}/**/*.{js,ts,tsx,jsx}\" --ignore-path .gitignore",
    "prettier:check": "prettier --check \"{src,templates,__tests__}/**/*.{js,ts,tsx,jsx}\" --ignore-path .gitignore",
    "test": "jest __tests__/package/* --config=jest.package.config.json  --no-cache",
    "templates:run": "jest __tests__/templates/* --config=jest.templates.config.json --no-cache --verbose",
    "test:debug": "node --inspect-brk jest __tests__/* --no-cache --runInBand",
    "test:coverage": "yarn test --coverage",
    "circular-deps": "madge --circular --extensions ts src",
    "prepare": "husky",
    "docs:generate-html": "typedoc --readme none --entryPointStrategy expand --out docs_html --disableSources --excludeInternal --excludeNotDocumented ./src",
    "docs:generate-md": "typedoc --plugin typedoc-plugin-markdown --readme none --entryPointStrategy expand --out docs_md --disableSources --excludeInternal --excludeNotDocumented ./src",
    "docs:generate-mdx": "python3 ./scripts/html_to_mdx.py --output docs_mdx docs_html",
    "docs:generate": "yarn docs:generate-html && yarn docs:generate-mdx",
    "type-check": "tsc --noEmit --project . && tsc --noEmit --project templates/ts/cli/tsconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "allure-jest": "^3.2.2",
    "allure-js-commons": "^3.2.2",
    "archiver": "^7.0.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "madge": "^8.0.0",
    "node-gyp": "^11.2.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.2",
    "wav-decoder": "^1.3.0",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@types/lodash.snakecase": "^4.1.9",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "koffi": "^2.10.1",
    "lodash.snakecase": "^4.1.1",
    "typedoc-plugin-markdown": "^4.8.0",
    "uuid": "^11.1.0"
  }
}
